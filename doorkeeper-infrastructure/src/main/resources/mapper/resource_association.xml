<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="org.limbo.doorkeeper.infrastructure.dao.mybatis.mapper.ResourceAssociationMapper" >

    <insert id="batchInsertIgnore" parameterType="org.limbo.doorkeeper.infrastructure.po.ResourceAssociationPO">
        INSERT ignore INTO resource_association (
            resource_id, parent_id
        ) VALUES
        <foreach collection="resourceAssociations" separator="," item="resourceAssociation">
            ( #{resourceAssociation.resourceId}, #{resourceAssociation.parentId} )
        </foreach>
    </insert>

</mapper>
